package com.masai;

import java.util.Scanner;

public class Parent {
	
	final int Num = 0;
	void method1() {
		System.out.println("Method belong to Parent class");
	}
 final void method2() {
	System.out.println("Method belong to Parent class");
	}
void method3() {
	System.out.println("Method belong to Parent class");
}

	public static void main(String[] args) {
		
		Parent p=new Child();
		Child C=(Child)p;
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a Number");
		int N=sc.nextInt();
		if(N>=1&&N<=10)
		{
		C.method1();
		C.method2();
		C.method3();
		C.method4();
		}
		else
		{
			System.out.println("Invalid Number");
		}

	}

}

final class Child extends Parent{
	@Override
	void method1() {
		System.out.println("Method belong to Child class");
	}
	@Override
void method3() {
	System.out.println("Method belong to Child class");
}
	void method4() {
		System.out.println("Method belong to Child class");
	}
	
}